# Flatten Array
# Simple Bruteforce method

def flatten_array(array):
    
    orgList = list(str(array)) # Converts to string and then list, for easier removal of specific characters
    revList = list(str(array))

    for x in revList:
        if x.isnumeric():  # Removes all numbers from revList
            revList.remove(x)
        else:
            continue

    newList = set(orgList).difference(set(revList))  #  Removes all non-numerics from original set of characters
    newList = [int(y) for y in newList if y.isnumeric()] # Converts all string numbers to integers

    return sorted(list((newList)))
